import cmath
a=float(input("a: "))
b=float(input("b: "))
c=float(input("c: "))
d=b**2 - 4*a*c
if d>0:
	r1=(-b+cmath.sqrt(d))/(2*a)
	r2=(-b-cmath.sqrt(d))/(2*a)
	print(f"The roots are: {r1.real:.2f} and {r2.real:.2f}")
elif d==0:
	r1=r2=(-b)/(2*a)
	print(f"The root is: {r1:.2f}")
else:
	r1=(-b+cmath.sqrt(d))/(2*a)
	r2=(-b-cmath.sqrt(d))/(2*a)
	real_part1=f"{r1.real:.2f}"
	imag_part1=f"{r1.imag:.2f}"
	real_part2=f"{r2.real:.2f}"
	imag_part2=f"{r2.imag:.2f}"
	if float(real_part1)==0.0:
		real_part1="-0.00"
	print(f"The roots are: {real_part1}+{imag_part1}j and {real_part2}{imag_part2}j")
		
